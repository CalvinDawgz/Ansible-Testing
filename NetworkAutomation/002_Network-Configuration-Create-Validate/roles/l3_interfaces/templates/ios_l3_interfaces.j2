{% if value.ipv4 is defined %}
{% for ipv4_addr in value.ipv4 %}
 ip address {{ ipv4_addr.IP | ipaddr('address')}} {{ ipv4_addr.IP | ipaddr('netmask') }}
{% endfor %}
{% endif %}
